{
  "public":
  [
    {
      "//": "send any type of request to our connected server",
      "method": "any",
      "path": "/*"
    }
  ],
  "private":
  [
    {
      "//": "Handle authentication",
      "method": "GET",
      "path": "/v2/token",
      "origin": "https://${ARTIFACTORYCR_HOSTNAME}",
      "auth": {
        "scheme": "basic",
        "username": "${ARTIFACTORYCR_USERNAME}",
        "password": "${ARTIFACTORYCR_PASSWORD}"
      }
    },
    {
      "//": "Query the V2 API",
      "method": "GET",
      "path": "/v2/*",
      "origin": "https://${ARTIFACTORYCR_HOSTNAME}"
    }
  ]
}
